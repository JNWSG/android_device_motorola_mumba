# Audio
ro.vendor.audio.sdk.fluencetype=none
ro.vendor.audio.sdk.ssr=false
persist.vendor.audio.fluence.voicecall=true
persist.vendor.audio.fluence.voicerec=false
persist.vendor.audio.fluence.speaker=true
vendor.audio.offload.buffer.size.kb=32
vendor.audio.offload.track.enable=true
vendor.audio.offload.multiaac.enable=true
vendor.audio.dolby.ds2.enabled=false
vendor.audio.dolby.ds2.hardbypass=false
vendor.audio.offload.gapless.enabled=true

# Bluetooth
ro.bluetooth.a2dp_offload.supported=true
persist.bluetooth.a2dp_offload.disabled=false
persist.bluetooth.a2dp_offload.cap=sbc-aac-aptx-aptxhd-ldac
ro.vendor.bluetooth.csip_qti=true

# Camera
ro.camera.enableCamera1MaxZsl=1
vendor.camera.aux.packagelist=org.codeaurora.snapcam,com.motorola.camera3,com.motorola.camera5,com.motorola.motocit
persist.vendor.camera.physical.num=3

# Crypto
ro.crypto.metadata_init_delete_all_keys.enabled=true

# Display
ro.surface_flinger.set_idle_timer_ms=500
ro.surface_flinger.set_touch_timer_ms=1000
ro.surface_flinger.use_content_detection_for_refresh_rate=true
ro.surface_flinger.enable_frame_rate_override=true
vendor.display.enable_optimize_refresh=0
vendor.display.disable_stc_dimming=1
vendor.display.enable_dpps_dynamic_fps=1

# DRM
drm.service.enabled=true

# FRP
ro.frp.pst=/dev/block/bootdevice/by-name/frp

# Gatekeeper
vendor.gatekeeper.disable_spu=true

# Graphics
ro.hardware.vulkan=adreno
ro.hardware.egl=adreno
ro.opengles.version=196610

# Media
media.c2.hal.selection=aidl
debug.stagefright.c2inputsurface=-1

# NFC
persist.vendor.nfc.stchipinfo=st54l

# Perf
ro.vendor.extension_library=libqti-perfd-client.so
ro.vendor.perf-hal.ver=3.0

# Radio
persist.vendor.radio.enableadvancedscan=true
persist.vendor.radio.ati_sim_not_pwdn=1
persist.vendor.radio.custom_ecc=1
persist.vendor.radio.sib16_support=1
persist.vendor.radio.data_ltd_sys_ind=1
ro.telephony.default_network=33

# Sensors
ro.vendor.sensors.mot_ltv=true
ro.vendor.sensors.mot_backlight=true
ro.vendor.sensors.prox_for_call=true
persist.vendor.sensors.mot_tap=true

# SoC
ro.soc.manufacturer=QTI
ro.board.platform=parrot

# USB
vendor.usb.rndis.func.name=gsi
vendor.usb.rmnet.func.name=gsi

# WiFi
wifi.aware.interface=wifi-aware0
